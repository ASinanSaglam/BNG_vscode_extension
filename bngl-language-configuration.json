{
    "comments": {
        "lineComment": "#",
    },
    "brackets": [
        ["{", "}"],
        ["(", ")"],
        ["[","]"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*begin\\s++[^\n|\\#]+\b",
            "end": "^\\s*end\\s+[^\n|\\#]+\b"
        }
    }
}