{
    "name": "bngl-grammar-vscode",
    "displayName": "BNGL-grammar-vscode",
    "description": "BNG language grammar for vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bngl",
            "aliases": ["BioNetGen Language", "bngl"],
            "extensions": [".bngl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bngl",
            "scopeName": "source.bngl",
            "path": "./syntaxes/bngl.tmLanguage.json"
        }]
    }
}