{
    "name": "bngl",
    "displayName": "BNGL-grammar",
    "description": "BNG language grammar for vscode",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Themes"
    ],
    "contributes": {
        "languages": [{
            "id": "bngl",
            "aliases": ["BioNetGen Language", "bng"],
            "extensions": [".bngl"],
            "configuration": "./bngl-language-configuration.json"
        }],
        "grammars": [{
            "language": "bngl",
            "scopeName": "source.bngl",
            "path": "./syntaxes/bngl.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "bngl",
                "path": "./snippets/bngl-snippets.json"
            }
        ],
        "themes": [{
            "label": "dark-bngl",
            "uiTheme": "vs-dark",
            "path": "./themes/dark-bngl.json"
        }]
    }
}